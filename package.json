{
  "name": "messenger",
  "version": "1.0.0",
  "description": "My webpack project",
  "source": "index.html",
  "scripts": {
    "test": "mocha --require ts-node/register --require ./mochaSetup.js './src/**/*.test.ts'",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "start": "npm run build && node server.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "targets": {
    "default": {
      "engines": {
        "browsers": ">=50%"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "4.18.1",
    "handlebars": "4.7.7",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/chai": "4.3.3",
    "@types/mocha": "10.0.0",
    "@types/proxyquire": "1.3.28",
    "@types/sinon": "10.0.13",
    "@types/uuid": "8.3.4",
    "chai": "4.3.6",
    "css-loader": "6.7.1",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "jsdom": "20.0.1",
    "mocha": "10.1.0",
    "proxyquire": "2.1.3",
    "sass": "1.55.0",
    "sass-loader": "13.1.0",
    "sinon": "14.0.1",
    "style-loader": "3.3.1",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  }
}
